<Attrs>
    <Attr label="Language" name="language" visible="false">
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="NO"/>
            </Function>
        </Scopes>
        <Type>
            <Enumeration defaultValue="SDL">
                <Entry value="Ada"/>
                <Entry value="CPP"/>
                <Entry value="C"/>
                <Entry value="GUI"/>
                <Entry value="SDL"/>
                <Entry value="Simulink"/>
                <Entry value="Blackbox_Device"/>
                <Entry value="VHDL"/>
                <Entry value="Pseudo function"/>
            </Enumeration>
        </Type>
    </Attr>
    <Attr label="Implementation" name="default_implementation" visible="false">
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="NO"/>
            </Function>
        </Scopes>
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
    </Attr>
    <Attr label="Function Type" name="is_type">
        <Type>
            <Enumeration defaultValue="NO">
                <Entry value="YES"/>
                <Entry value="NO"/>
            </Enumeration>
        </Type>
        <Scopes>
            <Function/>
        </Scopes>
    </Attr>
    <Attr label="Language" name="type_language">
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="YES"/>
            </Function>
        </Scopes>
        <Type>
            <Enumeration defaultValue="SDL">
                <Entry value="Ada"/>
                <Entry value="CPP"/>
                <Entry value="SDL"/>
            </Enumeration>
        </Type>
    </Attr>
    <Attr label="Instance of" name="instance_of">
        <Type>
            <String validator="[a-zA-Z0-9_]+"/>
        </Type>
        <Scopes>
            <Function>
                <AttrValidator name="is_type" value="NO"/>
            </Function>
        </Scopes>
    </Attr>
    <Attr label="Period (in ms)" name="period">
        <Type>
            <String defaultValue="1000" validator="\d+"/>
        </Type>
        <Scopes>
            <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Minimal inter-arrival time (in ms)" name="miat">
        <Type>
            <String defaultValue="0" validator="\d+"/>
        </Type>
        <Scopes>
            <Provided_Interface>
                <AttrValidator name="kind" value="Sporadic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Worst case execution time (in ms)" name="wcet">
        <Type>
            <String defaultValue="0" validator="\d+"/>
        </Type>
        <Scopes>
             <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
                <AttrValidator name="kind" value="Sporadic"/>
                <AttrValidator name="kind" value="Protected"/>
                <AttrValidator name="kind" value="Unprotected"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Message queue size" name="queue_size">
        <Type>
            <String defaultValue="1" validator="\d+"/>
        </Type>
        <Scopes>
            <Provided_Interface>
                <AttrValidator name="kind" value="Sporadic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Priority" name="priority">
        <Type>
            <String defaultValue="1" validator="\d+"/>
        </Type>
        <Scopes>
             <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
                <AttrValidator name="kind" value="Sporadic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Offset (ms)" name="dispatch_offset">
        <Type>
            <String defaultValue="0" validator="\d+"/>
        </Type>
        <Scopes>
             <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr label="Stack size (kb)" name="stack_size">
        <Type>
            <String defaultValue="50" validator="\d+"/>
        </Type>
        <Scopes>
             <Provided_Interface>
                <AttrValidator name="kind" value="Cyclic"/>
                <AttrValidator name="kind" value="Sporadic"/>
            </Provided_Interface>
        </Scopes>
    </Attr>
    <Attr name="Taste::InheritPI" type="property">
        <Type>
            <Boolean/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
    <Attr name="Taste::Autonamed" type="property">
        <Type>
            <Boolean defaultValue="True"/>
        </Type>
        <Scopes>
            <Required_Interface/>
        </Scopes>
    </Attr>
</Attrs>
